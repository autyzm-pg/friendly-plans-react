{
  "name": "friendly-plans",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "lint-fix": "tslint --fix --project .",
    "lint": "tslint --project .",
    "prettier:fix": "prettier --write \"**/*.{js,ts,tsx}\"",
    "prettier:check": "prettier --list-different \"**/*.{js,ts,tsx}\"",
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "test:ci": "jest --env=jsdom --silent --coverage --runInBand",
    "test:coverage": "yarn test --coverage",
    "test": "jest --env=jsdom --updateSnapshot --silent",
    "typescript:check": "tsc --noEmit",
    "pre-commit": "lint-staged"
  },
  "dependencies": {
    "@types/lodash.noop": "^3.0.6",
    "@types/lodash.sortby": "^4.7.6",
    "formik": "^1.5.7",
    "i18next": "^15.0.7",
    "lodash.noop": "^3.0.1",
    "lodash.sortby": "^4.7.0",
    "react": "16.8.6",
    "react-i18next": "^9.0.2",
    "react-native": "0.59.9",
    "react-native-config": "^0.11.7",
    "react-native-device-info": "^0.24.3",
    "react-native-elements": "1.2.0",
    "react-native-fast-image": "^5.1.2",
    "react-native-firebase": "^5.4.2",
    "react-native-floating-action": "^1.19.1",
    "react-native-gesture-handler": "^1.3.0",
    "react-native-image-picker": "^0.28.1",
    "react-native-splash-screen": "^3.2.0",
    "react-native-vector-icons": "^6.6.0",
    "react-navigation": "^3.11.0",
    "yup": "^0.27.0"
  },
  "devDependencies": {
    "@babel/core": "^7.3.4",
    "@babel/preset-typescript": "^7.3.3",
    "@types/enzyme": "^3.9.3",
    "@types/enzyme-adapter-react-16": "^1.0.5",
    "@types/jest": "^24.0.13",
    "@types/react": "^16.8.19",
    "@types/react-native": "^0.57.61",
    "@types/react-navigation": "^3.0.7",
    "@types/yup": "^0.26.15",
    "babel-jest": "^24.5.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "husky": "^3.0.5",
    "jest": "24.5.0",
    "jest-enzyme": "^7.0.2",
    "lint-staged": "^9.2.5",
    "metro-react-native-babel-preset": "^0.53.0",
    "prettier": "^1.15.3",
    "react-art": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-test-renderer": "16.8.6",
    "ts-jest": "^24.0.2",
    "tslint": "^5.12.0",
    "tslint-config-prettier": "^1.17.0",
    "tslint-react": "^3.6.0",
    "typescript": "^3.5.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --write",
      "tslint",
      "git add"
    ]
  }
}
